    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;BIODATA MAHASISWA&quot;" newline="True"/>
            <declare name="namaLengkap, namaPanggilan" type="String" array="False" size=""/>
            <input variable="namaLengkap"/>
            <input variable="namaPanggilan"/>
            <declare name="nim" type="Integer" array="False" size=""/>
            <input variable="nim"/>
            <declare name="prodi, kelas" type="String" array="False" size=""/>
            <input variable="prodi"/>
            <input variable="kelas"/>
            <declare name="umur" type="Integer" array="False" size=""/>
            <input variable="umur"/>
            <declare name="tinggiBadan, beratBadan, tigaAngkaAkhir, modulus" type="Real" array="False" size=""/>
            <input variable="tinggiBadan"/>
            <input variable="beratBadan"/>
            <assign variable="tigaAngkaAkhir" expression="nim % 1000"/>
            <assign variable="modulus" expression="tigaAngkaAkhir % 6"/>
            <output expression="&quot; Nama saya &quot; &amp; namaLengkap &amp; &quot;, biasa di panggil &quot; &amp; namaPanggilan &amp; &quot; dengan NIM &quot; &amp; nim &amp; &quot;, saya dari program studi &quot; &amp; prodi &amp; &quot; Kelas &quot; &amp; kelas &amp; &quot;. Saya berumur &quot; &amp; umur &amp; &quot; tahun, tinggi badan saya &quot; &amp; tinggiBadan &amp; &quot; cm dan berat badan saya &quot; &amp; beratBadan &amp; &quot; Kg.&quot;" newline="True"/>
            <output expression="&quot; Tiga angka terakhir NIM saya adalah &quot; &amp; tigaAngkaAkhir &amp; &quot; dimoduluskan dengan 6 sehingga modulusnya bernilai  &quot; &amp; modulus" newline="True"/>
        </body>
    </function>
